	<queryString>
		<![CDATA[SELECT
     compras.`N_Factura` AS compras_N_Factura,
     compras.`Nombre_Proveedor` AS compras_Nombre_Proveedor,
     compras.`Total_Compra` AS compras_Total_Compra,
     detallecompra.`Nombre_producto` AS detallecompra_Nombre_producto,
     productos.`Codigo` AS productos_Codigo,
     productos.`Nombre` AS productos_Nombre,
     proveedores.`Nombre` AS proveedores_Nombre,
     detallecompra.`Cantidad_Producto` AS detallecompra_Cantidad_Producto,
     detallecompra.`Valor_Unitario` AS detallecompra_Valor_Unitario,
     detallecompra.`Subtotal` AS detallecompra_Subtotal
FROM
     `compras` compras INNER JOIN `detallecompra` detallecompra ON compras.`IdCompra` = detallecompra.`id`
     INNER JOIN `productos` productos ON detallecompra.`Nombre_producto` = productos.`Codigo`
     INNER JOIN `proveedores` proveedores ON compras.`Nombre_Proveedor` = proveedores.`Nit`
WHERE
     compras.`Nombre_Proveedor` = $P{nombre}]]>
	</queryString>
