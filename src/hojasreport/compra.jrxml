	<queryString>
		<![CDATA[SELECT
     compras.`N_Factura` AS compras_N_Factura,
     compras.`Nombre_Proveedor` AS compras_Nombre_Proveedor,
     compras.`Total_Compra` AS compras_Total_Compra,
     detallecompra.`Nombre_producto` AS detallecompra_Nombre_producto
FROM
     `compras` compras INNER JOIN `detallecompra` detallecompra ON compras.`IdCompra` = detallecompra.`id`

