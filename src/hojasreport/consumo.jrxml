	<queryString>
		<![CDATA[SELECT
     consumos.`Fecha_Aplicacion` AS consumos_Fecha_Aplicacion,
     consumos.`Nombre_Cosecha` AS consumos_Nombre_Cosecha,
     detalleconsumos.`NombreProducto` AS detalleconsumos_NombreProducto
